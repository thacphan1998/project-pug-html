// slide main
.wrapContentMain
    .container-xl.containerItem
        .contentItem
            .titleItem 
                .wrapTitleItem 
                    h2.titleContentMain Dự án trung tâm thương mại toàn quốc 
            .row.rowItem
                .col-lg-8.ItemListcontent
                    .infoProject
                        .countsummary
                            p Hiện đang có&nbsp
                                span 1,498&nbsp
                                | dự án
                        .selectStatus 
                            select
                                option Mới nhất
                                option Mới cập nhật
                                option Giá cao nhất
                                option Giá thấp nhất
                    .listCardProject
                        each item in data_page.card_project
                            .itemCardProject(title= item.title)
                                .wrapimgCard
                                    a(href='detailNews.html', title= item.title)
                                        img(src= item.url_img_1 , alt= item.title )
                                        img(src= item.url_img_2 , alt= item.title )
                                        img(src= item.url_img_3 , alt= item.title )

                                .wrapInfoCard
                                    .tagStatus
                                        if item.statusUpdate
                                            label.titleStatusUpdate= item.statusUpdate
                                        else
                                            if item.status&&item.statusDes
                                                label.titleStatus= item.status
                                                span= item.statusDes
                                            else 
                                                label.titleStatus= item.status
                                    h4.tagTitle
                                        a(href='detailNews.html' title= item.title)= item.title
                                    .tagspecifications
                                        if item.spec_project.price
                                            span.price= item.spec_project.price

                                        if item.spec_project.acreage
                                            span.acreage= item.spec_project.acreage

                                        if item.spec_project.apartments
                                            span.apartments= item.spec_project.apartments
