                                            span= item.company.text

                .col-lg-4.ItemViewProject
                    .wrapVideo
                        .wrapTitle 
                            h4 Đánh giá dự án
                      
                        .wrapSlideMain
                            .contentSlideMain
                                .showSlideProjectNewList
                                    each item in data_page.slide_video
                                        .elementSlideMain
                                            .contentSlideMain
                                                a.wrapImgSlideMain.wrapImgResize.wrapVideoSlideMain(href='javascript:void(0);' title= item.title)
                                                    img(src= item.url_img alt= item.title_main)

                                                    .wrapTextSlideMain
                                                        .container-xl.containerTextSlideMain
                                                            .contentTextSlideMain
                                                                .titleSubSlideMain.titleSubVideo= item.title_sub
                                                    .wrapIcon 
                                                        .contentIcon
                                                            svg(xmlns='http://www.w3.org/2000/svg' viewBox='0 0 384 512')
                                                                path(fill='currentColor' d='M361 215C375.3 223.8 384 239.3 384 256C384 272.7 375.3 288.2 361 296.1L73.03 472.1C58.21 482 39.66 482.4 24.52 473.9C9.377 465.4 0 449.4 0 432V80C0 62.64 9.377 46.63 24.52 38.13C39.66 29.64 58.21 29.99 73.03 39.04L361 215z')
                    
                    .wrapNewsProject
                        .wrapTitle
                            h4 Tin tức dự án

                        ul.wrapNewsList
                            each item in data_page.project_news_list
                                li.itemNewsList
                                    .wrapImgNewList.wrapImgResize
                                        a(href= item.link title= item.title)
                                            img(src= item.url_img, alt= item.title)
                                    .wrapContentNewList
                                        a.wrapTitleNewList(href= item.link title= item.title)= item.title
                                        .timeNewList= item.time                                        
