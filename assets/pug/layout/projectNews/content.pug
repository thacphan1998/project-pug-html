.wrapContentPost
    .blockTitleContent
        ul.listPost
            each item in data_page.content_post 
                li.itemContentPost
                    h2.postTitleMain
                        a(href='detailNews.html', title= item.title)= item.title
                    .wrapImgPost 
                        a.imgContentPost.wrapImgResize(href='detailNews.html', title= item.content)
                            img(src= item.url_img, alt= item.title)
                    .wrapcontentDes 
                        span.timePost= item.time_post
                        p.contentPost= item.content
        
    
